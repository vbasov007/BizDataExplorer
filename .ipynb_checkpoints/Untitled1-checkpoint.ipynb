   "source": [
    "def tagged(tag: str, content: str, **attr) -> str:\n",
    "    res = '<'+tag\n",
    "    for item in attr:\n",
    "        res += ' '+item + '='+'\"'+attr[item]+'\"'\n",
    "    res += '>'\n",
    "    res += content\n",
    "    res += r'</' + tag + '>'\n",
    "\n",
    "    return res"
   ]
